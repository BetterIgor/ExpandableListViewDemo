<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.liyaguang.expandablelistviewdemo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.example.liyaguang.expandablelistviewdemo.view.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.liyaguang.expandablelistviewdemo.view.ScrollGridActivity" >
        </activity>
        <activity android:name="com.example.liyaguang.expandablelistviewdemo.view.ListListActivity" >
        </activity>
        <activity android:name="com.example.liyaguang.expandablelistviewdemo.view.ExpandableListViewActivity" >
        </activity>
        <activity android:name="com.example.liyaguang.expandablelistviewdemo.view.ExpandableGridViewActivity" >
        </activity>
    </application>

</manifest>